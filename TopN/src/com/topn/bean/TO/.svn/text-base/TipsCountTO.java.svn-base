public class TipsCountTO {

	private int systemTip;
	
	private int friendTip;
	
	private int moodTip;
	
	private int leaveMsgTip;

	public int getSystemTip() {
		return systemTip;
	}

	public void setSystemTip(int systemTip) {
		this.systemTip = systemTip;
	}

	public int getFriendTip() {
		return friendTip;
	}

	public void setFriendTip(int friendTip) {
		this.friendTip = friendTip;
	}

	public int getMoodTip() {
		return moodTip;
	}

	public void setMoodTip(int moodTip) {
		this.moodTip = moodTip;
	}

	public int getLeaveMsgTip() {
		return leaveMsgTip;
	}

	public void setLeaveMsgTip(int leaveMsgTip) {
		this.leaveMsgTip = leaveMsgTip;
	}
	
	
}
