package com.topn.bean.TO;

import java.io.Serializable;
import java.util.List;

import com.topn.bean.City;
import com.topn.bean.District;
import com.topn.bean.HighSchool;
import com.topn.bean.Province;
import com.topn.bean.University;

/**
 * 
 * 创建人 KingXt
 * 创建时间：2011-4-14 下午07:13:25
 * 
 * 缓存教育信息，这里只缓存一级菜单为了打开页面容易
 */
public class CacheEducationTO implements Serializable{

	//缓存所有的省份
	List<Province> pc;
	//缓存第一个身份的所有大学
	List<University> us;
	//缓存第一个省份的所有的城市
	List<City> cs;
	//缓存第一个市的所有区
	List<District> ds;
	//缓存第一个区的所有高中
	List<HighSchool> hs;
	public List<Province> getPc() {
		return pc;
	}
	public void setPc(List<Province> pc) {
		this.pc = pc;
	}
	public List<University> getUs() {
		return us;
	}
	public void setUs(List<University> us) {
		this.us = us;
	}
	public List<City> getCs() {
		return cs;
	}
	public void setCs(List<City> cs) {
		this.cs = cs;
	}
	public List<District> getDs() {
		return ds;
	}
	public void setDs(List<District> ds) {
		this.ds = ds;
	}
	public List<HighSchool> getHs() {
		return hs;
	}
	public void setHs(List<HighSchool> hs) {
		this.hs = hs;
	}
	
	
}
