	<script>
			function feedback(){
				if(jQuery.trim($("#message_to_us_area").val()) == ''){
					return;
				}
				var params = {
					'text' : jQuery.trim($("#message_to_us_area").val())
				};
				new TopNAjax().ajax({
					type: "post",
					cache: false,
					data : params,
					url: 'feedback.action',
					loadingAreaId : '',
					loading : false,
					beforeSend : function(){
						$(".submit_button").attr("disabled", "disabled");
					},
					success: function (msg) {
						alert("谢谢您的宝贵意见");
						$(".submit_button").attr("disabled", "");
						$("#message_to_us_area").val("");
			        }
				});
			}
			
			$(document).ready(function(){
				resolvePngInIe6();
				var param={
							timOut:null,
							spreadWidthInter:null,
							spreadHeightInter:null,
							person:null
							 };	
				$(document).click(function(){
					if($("#wrapOut").length>0)
					{
						closeBox();
					}
				});	
				function closeBox()
				{
					$("body").append(param.person.css("display","none"));
					$("#zxxBlank").remove();
					$("#wrapOut").remove();
				}
				function spreadWidth(obj)
				{
				clearInterval(param.spreadWidthInter);

					param.spreadWidthInter=setInterval(function()
						{
							if(obj.width()<525)
							{
								obj.width(obj.width()+30);
							}
								setIntroPosition();
						},5);
				}
				function spreadHieght(obj)
				{
				clearInterval(param.spreadHeightInter);

					param.spreadHeightInter=setInterval(function()
					{
						if(obj.height()<250)
						{
							obj.height(obj.height()+15);
						}						
						setIntroPosition();
					},5);
				}
				function setIntroPosition()
				{	
					var obj=$("#wrapOut");
						setPosition(obj);
						checkSize(obj);
				}
				function checkSize(obj)
				{
					if(obj.height()>=250)
					{
						clearInterval(param.spreadHeightInter);
					}
					if(obj.width()>=525)
					{
						clearInterval(param.spreadWidthInter);
						param.person.show();
						$("#loading_div").hide();
					}
				}
				function setBgTitle(title)
				{
					$("#zxxBlank").show();	
					$("#wrap_title_span").html(title);				
				}
				function setIntroBg(obj)
				{
					setBg($("#zxxBlank"));			
				}
				function showInfo(title)
				{
					var wrap='<div id="zxxBlank" onselectstart="return false;"></div><div class="wrap_out" id="wrapOut" style="width:525px;height:250px;"><div class="wrap_in" id="wrapIn"><div id="wrapBar" class="wrap_bar"><div class="wrap_title"><span id="wrap_title_span"></span></div><a  class="wrap_close" id="wrapClose">x</a></div>   <div class="wrap_body" id="wrapBody"> </div></div></div>';
					$("body").append(wrap);	
					setIntroBg();						
					var loading='<div id="loading_div" style="display:block;margin:auto;position: absolute;top: 40%;left: 0%;height: 25%;width: 100%;text-align: center;line-height: 0;"><img src="../pictures/lightbox-ico-loading.gif" /></div>';
					var wrapBody=$("#wrapBody");								
					var wrapOut = $("#wrapOut");					
					var wrapClose=$("#wrapClose");
					param.person.appendTo(wrapBody);					
					wrapOut.css("width","80px").css("height","40px");
					$(loading).appendTo(wrapBody);	
					setBgTitle(title);
					spreadWidth(wrapOut );
					spreadHieght(wrapOut );					
						wrapOut.unbind("click");
						wrapOut.click(function(event){
								event.stopPropagation();											
						});
						wrapClose.unbind("click");
						wrapClose.click(function(event){
							closeBox();		
							event.stopPropagation();							
						});
				}
				$("#us_photo_div ul li a").click(function(event){
						var id="#"+$(this).attr("name");
						param.person=$(id);
						showInfo($(this).attr("title"));
						event.stopPropagation();					
				});	
				$(".message_to_us_area").keyup(function(e){
					var currKey=0,
					e=e||event;
					currKey=e.keyCode||e.which||e.charCode;
					if(currKey==13)
					{
						
					}
				});	
				$(window).resize(function(){
					if(param.timeOut!=undefined)
					{
						clearTimeout(param.timeOut);
					}
					if($("#zxxBlank").length>0)
					{
						param.timeOut= setTimeout(function(){setIntroPosition();setIntroBg();},200);
					}
				});	
				$(window).scroll(function(){
					if(param.timeOut!=undefined)
					{
						clearTimeout(param.timeOut);
					}
					if($("#zxxBlank").length>0)
					{
						param.timeOut= setTimeout(function(){setIntroPosition();setIntroBg();},200);
					}
				});	

			});
			
			
			
		</script>
